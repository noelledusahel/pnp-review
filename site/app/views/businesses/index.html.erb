
<div class="container">
	<div class="profile">
		<div class=row>
			<% @businesses.each do |business| %>
					<div class="col-sm-4">
						<div class="biz-profile">
							<img src='<%=image_path("#{business.image_location}") %>' class="hover"/>
							<p id="<%=business.id%>"><%=business.name %></p>
						</div>	
					</div>	
			<% end  %>
		</div> 
	</div>
</div>

